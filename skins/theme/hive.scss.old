// ╔══════════════════════════╦═════════════════════════════════════════════════════════════════════════════[─]═[□]═[×]═╗
// ║ HEX to RGB               ║ This function converts HEX values to RGBa.                                              ║
// ╚══════════════════════════╩═════════════════════════════════════════════════════════════════════════════════════════╝

@function hexToRGB($hex) {
  @return red($hex), green($hex), blue($hex);
}

// ╔══════════════════════════╦═════════════════════════════════════════════════════════════════════════════[─]═[□]═[×]═╗
// ║ Root                     ║ These are the root variables.                                                           ║
// ╚══════════════════════════╩═════════════════════════════════════════════════════════════════════════════════════════╝

:root {

// Version
--version: "#{$G001}";
--skin: " #{$G002}";
--author: " #{$G003}";

// Fonts
--fonts-primary: #{$G004};
--fonts-secondary: #{$G005};
--fonts-tertiary: #{$G006};

--font-display: #{$G004} !important;

// Wallpaper
--wallpaper: url('#{$G007}');

// Width
--channel-width: #{$G008}px;
--members-width: #{$G009}px;
--profile-width: #{$G010}px;

// Text
--text-align: #{$G011};

// Radius
--radius-avatar: #{$G012}px;
--radius-button: #{$G013}px;
--radius-mention: #{$G014}px;
--radius-reaction: #{$G015}px;
--radius-primary: #{$G016}px;
--radius-secondary: #{$G017}px;
--radius-tertiary: #{$G018}px;

// Border
--border-width: #{$G019}px;
--border-opacity: #{$G020};

// Blur
--blur: #{$G021}px;
--blur-wallpaper: #{$G022}px;
--blur-titlebar: #{$G023}px;
--blur-backdrop: #{$G024}px;

// Transparency
--transparency-menu: #{$G025};
--transparency-titlebar: #{$G026};
--transparency-backdrop: #{$G027};
--transparency-background: #{$G028};

--transparency-primary: #{$G029};
--transparency-secondary: #{$G030};
--transparency-tertiary: #{$G031};

--transparency-hover: #{$G032};
--transparency-active: #{$G033};
--transparency-selected: #{$G034};

// Opacity
--selection-opacity: #{$G035};
--shadow-opacity: #{$G036};
--shadow-popout-opacity: #{$G037};

// Glasscord
--glasscord-win-blur: blurbehind;
--glasscord-macos-vibrancy: fullscreen-ui;
--glasscord-linux-blur: true;
--glasscord-titlebar: windows;
}

// ╔══════════════════════════╦═════════════════════════════════════════════════════════════════════════════[─]═[□]═[×]═╗
// ║ Dark                     ║ These are the variables for the Dark theme.                                             ║
// ╚══════════════════════════╩═════════════════════════════════════════════════════════════════════════════════════════╝

.theme-dark {

// Accent
--accent-primary: rgba(#{hexToRGB($D001)}, #{$G038});
--accent-secondary: rgba(#{hexToRGB($D002)}, #{$G039});
--accent-tertiary: rgba(#{hexToRGB($D003)}, #{$G040});

// Green Button
--green-primary: rgba(#{hexToRGB($D004)});
--green-secondary: rgba(#{hexToRGB($D005)});
--green-tertiary: rgba(#{hexToRGB($D006)});

// Yellow Button
--yellow-primary: rgba(#{hexToRGB($D007)});
--yellow-secondary: rgba(#{hexToRGB($D008)});
--yellow-tertiary: rgba(#{hexToRGB($D009)});

// Red Button
--red-primary: rgba(#{hexToRGB($D010)});
--red-secondary: rgba(#{hexToRGB($D011)});
--red-tertiary: rgba(#{hexToRGB($D012)});

// Purple Button
--purple-primary: rgba(#{hexToRGB($D013)});
--purple-secondary: rgba(#{hexToRGB($D014)});
--purple-tertiary: rgba(#{hexToRGB($D015)});

// Background
--background-primary: rgba(#{hexToRGB($D016)}, var(--transparency-primary));
--background-secondary: rgba(#{hexToRGB($D017)}, var(--transparency-secondary));
--background-tertiary: rgba(#{hexToRGB($D018)}, var(--transparency-tertiary));
--background-secondary-alt: rgba(#{hexToRGB($D018)}, var(--transparency-tertiary));

--background-accent: rgba(#{hexToRGB($D001)});
--background-floating: rgba(#{hexToRGB($D018)}, var(--transparency-menu));

--background-mobile-primary: rgba(#{hexToRGB($D016)});
--background-mobile-secondary: rgba(#{hexToRGB($D017)});

--background-modifier-hover: rgba(#{hexToRGB($D002)});
--background-modifier-active: rgba(#{hexToRGB($D001)});
--background-modifier-selected: rgba(#{hexToRGB($D001)});
--background-modifier-accent: rgba(#{hexToRGB($D016)});

--background-mentioned: rgba(#{hexToRGB($D001)}, 0.1);
--background-mentioned-hover: rgba(#{hexToRGB($D001)}, 0.2);

--background-message-hover: rgba(#{hexToRGB($D018)}, 0.4);
--background-help-warning: rgba(#{hexToRGB($D001)});
--background-help-info: rgba(#{hexToRGB($D002)});

// Toast
--toast-background: rgba(#{hexToRGB($D017)}, var(--transparency-secondary));
--toast-header: rgba(#{hexToRGB($D016)}, var(--transparency-primary));
--toast-contents: rgba(#{hexToRGB($D018)}, var(--transparency-tertiary));
--toast-border: var(--background-secondary);
--toast-box-shadow: rgba(0, 0, 0, 0.400);

// Header
--header-primary: rgba(#{hexToRGB($D019)});
--header-secondary: rgba(#{hexToRGB($D021)});

// Text
--text-normal: rgba(#{hexToRGB($D020)});
--text-muted: rgba(#{hexToRGB($D022)});
--text-link: rgba(#{hexToRGB($D001)});

// Channels
--channels-default: rgba(#{hexToRGB($D021)});

// Interactive
--interactive-normal: rgba(#{hexToRGB($D020)});
--interactive-hover: rgba(#{hexToRGB($D019)});
--interactive-active: rgba(#{hexToRGB($D019)});
--interactive-muted: rgba(#{hexToRGB($D022)});

// Spotify Color
--spotify-color: rgba(#{hexToRGB($D001)});

// Scrollbar
--scrollbar-thin-thumb: rgba(#{hexToRGB($D016)});
--scrollbar-thin-track: rgba(#{hexToRGB($D018)});
--scrollbar-auto-thumb: rgba(#{hexToRGB($D016)});
--scrollbar-auto-track: rgba(#{hexToRGB($D018)});
--scrollbar-auto-scrollbar-color-thumb: rgba(#{hexToRGB($D016)});
--scrollbar-auto-scrollbar-color-track: rgba(#{hexToRGB($D016)});

// Deprecated
--deprecated-card-bg: var(--background-tertiary);
--deprecated-card-editable-bg: var(--background-secondary);
--deprecated-store-bg: var(--background-secondary);
--deprecated-quickswitcher-input-background: var(--background-tertiary);
--deprecated-quickswitcher-input-placeholder: var(--text-normal);
--deprecated-text-input-bg: var(--background-tertiary);

// Elevation
--elevation-stroke: none;
--elevation-low: none;
--elevation-medium: none;
--elevation-high: none;

// Other
--logo-primary: var(--accent-primary);
--focus-primary: var(--accent-primary);
--radio-group-dot-foreground: var(--accent-primary);
--channeltextarea-background: rgba(#{hexToRGB($D019)});
--activity-card-background: var(--background-secondary);
--textbox-markdown-syntax: rgba(#{hexToRGB($D019)});

// Selection
--selection-text: rgba(#{hexToRGB($D025)});
--selection-background: rgba(#{hexToRGB($D026)}, var(--selection-opacity));

// Custom
--titlebar: rgba(#{hexToRGB($D018)}, var(--transparency-titlebar));

--background: rgba(#{hexToRGB($D016)}, var(--transparency-background));
--shadows: 0px 2px 8px 2px rgba(#{hexToRGB($D023)}, var(--shadow-opacity));
--shadows-popout: 0px 2px 8px 2px rgba(#{hexToRGB($D023)}, var(--shadow-popout-opacity));
--backdrop: rgba(#{hexToRGB($D027)}, var(--transparency-backdrop));

--highlight-hover: rgba(#{hexToRGB($D018)}, var(--transparency-hover));
--highlight-active: rgba(#{hexToRGB($D018)}, var(--transparency-active));
--highlight-selected: rgba(#{hexToRGB($D018)}, var(--transparency-selected));

--accent-active: rgba(#{hexToRGB($D001)}, var(--transparency-hover));
--accent-hover: rgba(#{hexToRGB($D002)}, var(--transparency-active));
--accent-selected: rgba(#{hexToRGB($D003)}, var(--transparency-selected));

--normal-primary: rgba(#{hexToRGB($D016)});
--normal-secondary: rgba(#{hexToRGB($D017)});
--normal-tertiary: rgba(#{hexToRGB($D018)});

--border: var(--border-width) solid rgba(#{hexToRGB($D024)}, var(--border-opacity));

// Link preview
--linkpreview-background: transparent;
--linkpreview-color: #FFFFFF;
--linkpreview-border: none;

// Stream
--stream-active: rgba(#{hexToRGB($D013)}, var(--transparency-hover));
--stream-hover: rgba(#{hexToRGB($D014)}, var(--transparency-hover));
--stream-selected: rgba(#{hexToRGB($D015)}, var(--transparency-hover));

// Accent Text
--accent-text-primary: rgba(#{hexToRGB($D028)});
--accent-text-secondary: rgba(#{hexToRGB($D029)});
}

